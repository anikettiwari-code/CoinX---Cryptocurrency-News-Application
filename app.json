{
  "expo": {
    "name": "CoinX",
    "slug": "CoinXApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/crypto_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/crypto_icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Aniket.coinx"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/crypto_icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3e54e7f1-29cf-428f-86f4-08a4784e7b65"
      }
    }
  }
}
